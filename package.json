{
  "name": "reactstorybook",
  "version": "1.0.0",
  "description": "This is Storybook",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:storybook": "storybook start --port 6006 & jest",
    "storybook": "npm run lint:fix && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "dev": "node_modules/.bin/webpack-dev-server --mode=development --config webpack.dev.js",
    "build": "rm -rf build && node_modules/.bin/webpack --mode production --config webpack.prod.js",
    "lint": "eslint --config ./.eslintrc.json 'src/**/*.{js,jsx,ts,tsx}' --ext .jsx --ext .js",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "6.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-storyshots": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/react": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
